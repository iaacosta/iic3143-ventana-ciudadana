<div class="login">
  <h2 class="titulos titulos--medium">Inicio de sesión</h2>
  <form class="form" action="/session" method="post">
    <input name="username" type="text" class="form__input" placeholder="Nombre de usuario">
    <input name="password" type="password" class="form__input" placeholder="Contraseña">
    <input type="submit" class="button form__button" value="Iniciar sesión">
  </form>
</div>
